{
  "name": "@benalsam/admin-backend",
  "version": "1.0.0",
  "description": "Benalsam Admin Backend API",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist-esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "dist-esm"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "npm run build && npm run dev:watch",
    "dev:watch": "tsc --watch -p tsconfig.json",
    "clean": "rm -rf dist dist-esm",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "prepare": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "typescript",
    "types",
    "benalsam",
    "shared"
  ],
  "author": "Benalsam Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "@elastic/elasticsearch": "^8.11.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^20.8.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "private",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angache/benalsam-shared-types.git"
  }
} 