# Backend Configuration
EXPO_PUBLIC_ADMIN_BACKEND_URL=http://192.168.1.6:3002
EXPO_PUBLIC_API_BASE_URL=http://192.168.1.6:3002/api/v1

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# App Configuration
EXPO_PUBLIC_APP_NAME=Benalsam
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_ENV=development

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_DEBUG_MODE=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# Analytics Configuration
EXPO_PUBLIC_ANALYTICS_ENDPOINT=http://192.168.1.6:3002/api/v1/analytics

# Image Configuration
EXPO_PUBLIC_MAX_IMAGE_SIZE=10485760
EXPO_PUBLIC_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/webp

# Development Configuration
EXPO_PUBLIC_DEV_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug
