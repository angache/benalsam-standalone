{
  "rabbit_version": "4.1.3",
  "users": [
    {
      "name": "benalsam",
      "password": "benalsam123",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "benalsam",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "benalsam.listings",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "benalsam.system",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "elasticsearch.sync",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "listing.status.changes",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    },
    {
      "name": "system.health",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type": "classic"
      }
    }
  ],
  "bindings": [
    {
      "source": "benalsam.listings",
      "vhost": "/",
      "destination": "elasticsearch.sync",
      "destination_type": "queue",
      "routing_key": "listing.#",
      "arguments": {}
    },
    {
      "source": "benalsam.listings",
      "vhost": "/",
      "destination": "listing.status.changes",
      "destination_type": "queue",
      "routing_key": "listing.status.#",
      "arguments": {}
    },
    {
      "source": "benalsam.system",
      "vhost": "/",
      "destination": "system.health",
      "destination_type": "queue",
      "routing_key": "health.#",
      "arguments": {}
    }
  ]
}
