# Server Configuration
NODE_ENV=development
PORT=3012
API_VERSION=v1

# Database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# RabbitMQ
RABBITMQ_URL=amqp://benalsam:benalsam123@localhost:5672
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=benalsam
RABBITMQ_PASSWORD=benalsam123

# JWT
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=24h

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Service Discovery
CONSUL_HOST=localhost
CONSUL_PORT=8500

# Queue Configuration
QUEUE_BATCH_SIZE=5
QUEUE_MAX_RETRIES=3
QUEUE_PROCESSING_TIMEOUT=30000
QUEUE_STUCK_JOB_TIMEOUT=30000


# CORS
CORS_ORIGIN=http://localhost:3000,http://localhost:3002,http://localhost:3003
