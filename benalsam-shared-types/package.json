{
  "name": "benalsam-shared-types",
  "version": "1.0.9",
  "description": "Shared TypeScript types and services for Benalsam mobile and web applications",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist-esm/index.js",
    "require": "./dist/index.js"
  },
  "files": [
    "dist",
    "dist-esm"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "dev": "npm run build && npm run dev:watch",
    "dev:watch": "tsc --watch -p tsconfig.json",
    "clean": "rm -rf dist dist-esm",
    "prebuild": "npm run clean",
    "postinstall": "echo 'Skipping build for now'",
    "prepare": "echo 'Skipping prepare for Docker build'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "typescript",
    "types",
    "benalsam",
    "shared"
  ],
  "author": "Benalsam Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.50.3",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "@types/node": "^20.8.3",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
